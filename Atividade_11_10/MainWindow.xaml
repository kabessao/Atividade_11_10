<Window x:Class="Atividade_11_10.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Atividade_11_10"
        mc:Ignorable="d"
        Title="MainWindow" SizeToContent="Height" Width="200">
    <StackPanel Margin="5" >
        <TextBlock Text="Teste de combobox:" FontWeight="Bold"/>
        <ComboBox SelectedIndex="0"
                  x:Name="cboxTeste">
            <ComboBoxItem>São Paulo</ComboBoxItem>
            <ComboBoxItem>Rio de Janeiro</ComboBoxItem>
            <ComboBoxItem>Minas Gerais</ComboBoxItem>
        </ComboBox>
        <Button Content="testar" Click="Testar"/>

    </StackPanel>
</Window>
